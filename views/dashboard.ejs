<style>
    body {margin: 0;}
    .maincontainer {
      width: 100%;
      height: 95%;
      display: flex;
      font-family: "Poppins";
      flex-direction: column;
    }
    .box {
      width: 80%;
      height: auto;
      background-color: #fff;
      border-radius: 10px;
      padding: 15px;
      border: 1px solid grey;
    }
    .navbarstyle{
        width: 100%;
        height: 50px;
        background: #84817a;
    }
   .leftBox{
    width: 270px;
    height: 400px;
    background:#aaa69d ;
    margin: 20px;
    padding: 10px;
    border-radius: 20px;
   }
   
  </style>
  <html>
    <head>
      <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet" />
    </head>
    <body>
      <div class="maincontainer">
        <div class="navbarstyle">
          <div><img src="/images/logo.png" class="logostyle" style="width: 70px;height: 40px;margin:5px; padding-left: 10px;"></div>
        </div>

        <div style="display: flex;">
          <div class="leftBox">
            <div style="display: flex; justify-content: center;flex-direction: column;">
              <img src="/images/<%=data.picture %>" style="align-self: center; width: 100px; height: 100px; border-radius: 50px;">
              <div style="color: #fff; font-weight: 500px; letter-spacing: 1px; font-size:11px ;align-self: center;">
                <%=data.adminname %></div>
              <div style="color: #fff; font-weight: 500px; letter-spacing: 1px; font-size:11px ;align-self: center;"> 
                <%=data.emailid %>
              </div>
              <div style="color: #fff; font-weight: 500px; letter-spacing: 1px; font-size:11px ;align-self: center;"> 
                +91-<%=data.mobileno %>
              </div>
              <div style="margin-top: 20px; color: #fff; font-weight: bold; font-size:14px ;align-self: center;"> 
                <a href="/food/display_all_food" target="mw" style="text-decoration:none; margin-top: 20px; color: #fff; font-weight: bold; font-size:14px ;align-self: center;">Food List</a>
              </div>
              <div style="margin-top: 20px; color: #fff; font-weight: bold; font-size:14px ;align-self: center;"> 
                <a href="/food/food_interface" target="mw" style="text-decoration:none; margin-top: 20px; color: #fff; font-weight: bold; font-size:14px ;align-self: center;">Add List</a>
              </div>
              <div style="margin-top: 20px; color: #fff; font-weight: bold; font-size:14px ;align-self: center;"> 
                <a href="/food/About_Us" target="mw" style="text-decoration:none; margin-top: 20px; color: #fff; font-weight: bold; font-size:14px ;align-self: center;">About Us</a>
              </div>
              <div style="margin-top: 20px; color: #fff; font-weight: bold; font-size:14px ;align-self: center;"> 
                <a href="/food/display_all_food" target="mw" style="text-decoration:none; margin-top: 20px; color: #fff; font-weight: bold; font-size:14px ;align-self: center;">update food </a>
              </div>
              <div style="margin-top: 20px; color: #fff; font-weight: bold; font-size:14px ;align-self: center;"> 
                <a href="/admin/admin_logout" style="text-decoration:none; margin-top: 20px; color: #fff; font-weight: bold; font-size:14px ;align-self: center;">Logout</a>
              </div>
            </div>
          </div>
          <div class="rightBox">
            <iframe frameborder="0" width="1400" height="700" name="mw" style="padding:10px; margin: 5px; border-radius: 20px;"></iframe>
          </div>

        </div>

      </div>
    </body>
  </html>